{%
  set body_classes = [
    logged_in ? 'user-logged-in',
    not root_path ? 'path-frontpage' : 'path-' ~ root_path|clean_class,
    node_type ? 'page-node-type-' ~ node_type|clean_class,
    db_offline ? 'db-offline',
  ]
%}
<!DOCTYPE html>
<html{{ html_attributes }}>
  <head>
    {#<head-placeholder token="{{ placeholder_token }}">#}
    <title>{{ head_title|safe_join(' | ') }}</title>
    {% include '/themes/Blank/templates/layout/includes/css.html.twig' %}
  </head>
  <body {{ attributes.addClass(body_classes) }}>

    {# Keyboard navigation/accessibility link to main content section in page.html.twig. #}
    {#<a href="#main-content" class="visually-hidden focusable skip-link"> {{ 'Skip to main content'|t }} </a>#}
    {{ page_top }}

    {% include '/themes/Blank/templates/layout/includes/header.html.twig' %}
    <div class="container">
      {{ page }}
    </div>
    {{ page_bottom }}
    {% include '/themes/Blank/templates/layout/includes/footer.html.twig' %}
    {% include '/themes/Blank/templates/layout/includes/js.html.twig' %}
  </body>
</html>
